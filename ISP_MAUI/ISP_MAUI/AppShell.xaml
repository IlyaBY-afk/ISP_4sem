<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="ISP_MAUI.AppShell"
    xmlns:view="clr-namespace:ISP_MAUI.Views">

    <ShellContent
        Title="Calculator"
        ContentTemplate="{DataTemplate view:CalculatorView}"
        Route="CalculatorPage"/>
    
    <ShellContent
        Title="Progress Bar"
        ContentTemplate="{DataTemplate view:ProgressBar}"
        Route="ProgressBar"/>
        
    <ShellContent
        Title="Data Base"
        ContentTemplate="{DataTemplate view:DemoDb}"
        Route="Data Base"/>
    
    <ShellContent
        Title="Currency"
        ContentTemplate="{DataTemplate view:Currency}"
        Route="Currency"/>
    
</Shell>
